		"/organizations": {
			"get": {
				"summary": "Organizations",
				"description": "Get Organizations",
				"operationId": "organizations",
				"parameters": [{
					"in": "query",
					"name": "categories",
					"description": "Filter by categories (comma separated, ANDd together) e.g. categories=Automotive,Public Transportation",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "category_uuids",
					"description": "Filter by one or more Categories. Separate multiple UUIDs with commas. When passed, multiple UUIDs are logically ANDd together",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "domain_name",
					"description": " Text search of an Organizations domain_name (e.g. www.google.com)",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "locations",
					"description": "Filter by location names (comma separated, ANDd together) e.g. locations=California,San Francisco",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "name",
					"description": "Full text search limited to name and aliases",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "organization_types",
					"description": "Filter by one or more types. Multiple types are separated by commas. Available types are company, investor, school, and group. Multiple organization_types are logically ANDd.",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "page",
					"description": "Page number of the results to retrieve.",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "query",
					"description": "Full text search of an Organizations name, aliases (i.e. previous names or also known as), and short description",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "sort_order",
					"description": "The sort order of the collection. Options are created_at ASC, created_at DESC, updated_at ASC, and updated_at DESC",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "updated_since",
					"description": "Timestamp When provided, restricts the result set to Organizations where updated_at >= the passed value",
					"type": "string",
					"format": "string"
				},
				{
					"in": "query",
					"name": "user_key",
					"description": "Possible values are: 7a582a92032069b4f775a15b1acbe256",
					"type": "string",
					"format": "string"
				}],
				"responses": {
					"200": {
						"description": "OK"
					}
				},
				"tags": ["Organizations"],
				"security": []
			}
		},
